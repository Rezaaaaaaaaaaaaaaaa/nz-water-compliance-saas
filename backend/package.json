{
  "name": "compliance-saas-backend",
  "version": "1.0.0",
  "description": "NZ Water Utility Compliance Management Backend",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "check-ports": "node ../scripts/check-ports.js 3001",
    "dev": "tsx watch src/server.ts",
    "dev:safe": "npm run check-ports && npm run dev",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:safe": "npm run check-ports && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.cjs",
    "test:all": "npm run test && npm run test:integration",
    "health:monitor": "tsx tools/health-monitor.ts",
    "health:check-once": "tsx tools/health-monitor.ts --once",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:seed:test": "tsx prisma/seed.ts",
    "test:db:setup": "node scripts/setup-test-db.js",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.ts$",
    "start:test": "NODE_ENV=test node dist/server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@aws-sdk/client-cloudwatch": "^3.932.0",
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/client-ses": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/websocket": "^11.2.0",
    "@prisma/client": "^5.7.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.0.0",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "fastify": "^5.6.2",
    "ioredis": "^5.3.2",
    "pino": "^10.1.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^30.2.0",
    "pg": "^8.16.3",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
